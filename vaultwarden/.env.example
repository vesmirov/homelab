# Vaultwarden environment variables
# Copy this file to .env and fill in the values

# Domain for the service (used for email links, etc.)
DOMAIN=https://vault.vesmirov.com

# Admin panel token (generate with: openssl rand -base64 48)
# Recommendation - use argon2id (about: https://en.wikipedia.org/wiki/Argon2)
ADMIN_TOKEN=

# Registration settings
SIGNUPS_ALLOWED=true
INVITATIONS_ALLOWED=false

# Other settings
WEBSOCKET_ENABLED=true
LOG_FILE=/data/vaultwarden.log
