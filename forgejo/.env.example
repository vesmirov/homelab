# Forgejo environment variables
# Copy this file to .env and fill in the values

# User/Group ID (run `id` to get your values)
USER_UID=1000
USER_GID=1000

# Server settings
ROOT_URL=http://localhost:8082
SSH_PORT=2222
SSH_DOMAIN=localhost

# Registration (set to true for initial setup, then false)
DISABLE_REGISTRATION=false
