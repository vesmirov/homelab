# Forgejo environment variables
# Copy this file to .env and fill in the values

# Network binding (use 127.0.0.1 for local only, or specific IP)
HOST_IP=127.0.0.1

# Server settings
ROOT_URL=http://localhost:8082
SSH_PORT=2222
SSH_DOMAIN=localhost

# Registration (set to true for initial setup, then false)
DISABLE_REGISTRATION=true